<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cormac.activities">

    <application
        android:allowBackup="true"
        android:icon="@drawable/scorecardiconv2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PlayerNames" />
        <activity android:name=".Front9" />
        <activity android:name=".FinalScore" />
        <activity android:name=".EnterCourse" />
        <activity android:name=".Start">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SelectCourse"></activity>
    </application>

</manifest>